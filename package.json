{
  "name": "@fanoutio/grip",
  "version": "3.1.0",
  "author": "Fanout, Inc. <info@fanout.io>",
  "description": "GRIP Interface Library",
  "packageManager": "yarn@1.22.15",
  "contributors": [
    {
      "name": "Katsuyuki Ohmuro",
      "email": "harmony7@pex2.jp"
    },
    {
      "name": "Konstantin Bokarius",
      "email": "kon@fanout.io"
    }
  ],
  "main": "./build/src/main.js",
  "module": "./build/esm/main.js",
  "files": [
    "browser/**/*",
    "build/**/*",
    "types/**/*"
  ],
  "types": "./build/src/main.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "yarn test && yarn lint && yarn build-package && yarn build-browser",
    "build-browser": "yarn build-browser:clean && yarn build-browser:build-dev && yarn build-browser:build-prod",
    "build-browser:clean": "rimraf browser",
    "build-browser:build-dev": "NODE_ENV=development webpack",
    "build-browser:build-prod": "NODE_ENV=production webpack -p",
    "build-package": "yarn build-package:clean && yarn build-package:compile",
    "build-package:clean": "rimraf build",
    "build-package:compile": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint src",
    "test": "ts-mocha -p tsconfig.json tests/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanout/js-grip.git"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "grip",
    "fanout",
    "fanoutpub",
    "realtime",
    "push",
    "pubcontrol",
    "publish"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "mocha": "^10.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "debug": "^4.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "jspack": "0.0.4"
  }
}
