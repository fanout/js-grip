{
  "name": "@fanoutio/grip",
  "version": "3.3.1",
  "type": "module",
  "author": "Fanout, Inc. <info@fanout.io>",
  "description": "GRIP Interface Library",
  "contributors": [
    {
      "name": "Katsuyuki Omuro",
      "email": "komuro@fastly.com"
    },
    {
      "name": "Konstantin Bokarius",
      "email": "kon@fanout.io"
    }
  ],
  "main": "./build/index.js",
  "files": [
    "browser/**/*",
    "build/**/*",
    "types/**/*"
  ],
  "types": "./build/src/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run test && npm run build-package && npm run build-browser",
    "build-browser": "npm run build-browser:clean && npm run build-browser:build-dev && npm run build-browser:build-prod",
    "build-browser:clean": "rimraf browser",
    "build-browser:build-dev": "NODE_ENV=development webpack",
    "build-browser:build-prod": "NODE_ENV=production webpack",
    "build-package": "npm run build-package:clean && npm run build-package:compile",
    "build-package:clean": "rimraf build",
    "build-package:compile": "tsc --build tsconfig.build.json",
    "coverage": "c8 npm test",
    "test": "npm run test:unit",
    "test:unit": "node --test --no-warnings=ExperimentalWarning --loader ts-node/esm test/unit/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanout/js-grip.git"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "grip",
    "fanout",
    "fanoutpub",
    "realtime",
    "push",
    "pubcontrol",
    "publish"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20",
    "buffer": "^6.0.3",
    "c8": "^8.0.1",
    "crypto-browserify": "^3.12.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "url": "^0.11.3",
    "util": "^0.12.5",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "debug": "^4.3.4",
    "jsonwebtoken": "^9.0.2",
    "jspack": "0.0.4"
  },
  "engines": {
    "node": ">= 18"
  }
}
