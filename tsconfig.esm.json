    "downlevelIteration": true,
    "rootDir": "src",
    "outDir": "build/esm",
    "tsBuildInfoFile": "build/esm/tsconfig.esm.tsbuildinfo"
  },
  "include": [
    "src/**/*.ts",
    "types/**/*.ts"
  ]
}
